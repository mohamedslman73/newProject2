{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "bacon/bacon-qr-code": "^1.0",
        "barryvdh/laravel-debugbar": "^2.4",
        "bican/roles": "2.1.*",
        "doctrine/instantiator": "^1.0",
        "elasticquent/elasticquent": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "imi/laravel-transsid": "dev-master",
        "intervention/image": "dev-master",
        "jenssegers/agent": "^2.5",
        "laravel/framework": "5.4.*",
        "laravel/passport": "^4.0",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.4.0",
        "maatwebsite/excel": "~2.1.0",
        "maknz/slack": "^1.7",
        "pragmarx/tracker": "^3.2",
        "rmccue/requests": "^1.7",
        "spatie/laravel-activitylog": "1.16.0",
        "spatie/laravel-backup": "4.*",
        "spatie/laravel-json-api-paginate": "^1.2",
        "spatie/laravel-permission": "^2.4",
        "spatie/laravel-responsecache": "3.2.0",
        "yajra/laravel-datatables-oracle": "^7.5",
        "zizaco/entrust": "^1.8"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
